.swiper-tab{
    width: 100%;
    border-bottom: 2rpx solid #ccc;
    text-align: center;
    height: 88rpx;
    line-height: 88rpx;
    display: flex;
    flex-flow: row;
    justify-content: space-between;
}
.swiper-tab-item{
    width: 30%; 
    color:#434343;
    font-size: 33rpx;
} 
.active{
    color:#F65959;
    border-bottom: 4rpx solid #F65959;
}
swiper{
  width: 100%;
  height: 100vh;
  text-align: center;
}

.item_not_data_view{
  width: 100%;
  height: 100vh;
}

.item_not_data_img{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}

.item_cotent{
  width: 100%;
  height: 100vh;
  overflow-y: scroll;
}

.item_cotent::-webkit-scrollbar {
    display: none;
}