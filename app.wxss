/**app.wxss**/
@import "./assets/iconfont/iconfont.wxss";
@import "./assets/css/public.wxss";

.swiper {
  height: 400rpx;
  margin-bottom: 15rpx;
}

.swiper image {
  height: 100%;
  width: 100%;
  
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.view_line_h{
  width: 100%;
  height: 1px;
  background: #EDEDED;
} 

.view_line_v{
  width: 1px;
  height: 100%;
  background: #EDEDED;
} 

.border_view{
  box-sizing: border-box; 
  -webkit-box-sizing: border-box; 
  -moz-box-sizing: border-box; 
}
