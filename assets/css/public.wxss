.clearfix:after {
  display: block;
  content: "";
  clear: both;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.flex_box {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  display: -webkit-flex;
}

.flex_bt {
  -webkit-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
}

.lineMin {
  width: 100%;
  height: 1rpx;
  background: #eee;
}

.lineMax {
  width: 100%;
  height: 15rpx;
  background: #eee;
}

.lineMMax {
  text-align: center;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  color: #999;
  background: #eee;
  font-size: 22rpx;
  position: relative;
}

.lineMMax:after {
  content: "";
  position: absolute;
  top: 43rpx;
  right: 210rpx;
  width: 130rpx;
  height: 2rpx;
  background: #999;
}
.lineMMax::before {
  content: "";
  position: absolute;
  top: 43rpx;
  left: 210rpx;
  width: 130rpx;
  height: 2rpx;
  background: #999;
}

